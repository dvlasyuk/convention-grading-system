@page "/events/{eventId}/participants"
@using Microsoft.AspNetCore.Components.Web
@using MudBlazor

<PageTitle>Участники мероприятия @EventId</PageTitle>
<MudText Typo="Typo.h6">Участники мероприятия @EventId</MudText>

@code {

    /// <summary>
    /// Идентификатор мероприятия в рамках конкурса.
    /// </summary>
    [Parameter]
    public string? EventId { get; set; }
}
