@page "/events/{eventId}/participant-feedbacks"
@using Microsoft.AspNetCore.Components.Web
@using MudBlazor

<PageTitle>Отзывы участников о мероприятии @EventId</PageTitle>
<MudText Typo="Typo.h6">Отзывы участников о мероприятии @EventId</MudText>

@code {

    /// <summary>
    /// Идентификатор мероприятия в рамках конкурса.
    /// </summary>
    [Parameter]
    public string? EventId { get; set; }
}
