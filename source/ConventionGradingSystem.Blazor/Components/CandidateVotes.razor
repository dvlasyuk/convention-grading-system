@page "/candidates/{candidateId}/votes"
@using Microsoft.AspNetCore.Components.Web
@using MudBlazor

<PageTitle>Голоса за кандидата зрительского голосования @CandidateId</PageTitle>
<MudText Typo="Typo.h6">Голоса за кандидата зрительского голосования @CandidateId</MudText>

@code {

    /// <summary>
    /// Идентификатор кандидата зрительского голосования.
    /// </summary>
    [Parameter]
    public string? CandidateId { get; set; }
}
