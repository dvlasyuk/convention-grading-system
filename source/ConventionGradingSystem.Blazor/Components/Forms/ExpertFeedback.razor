@page "/forms/expert-feedback/{eventId}"
@layout BaseLayout

@using ConventionGradingSystem.Blazor.Components.Shared
@using Microsoft.AspNetCore.Components.Web
@using MudBlazor

<PageTitle>Форма отзыва эксперта о мероприятии @EventId</PageTitle>
<MudText Typo="Typo.h6">Форма отзыва эксперта о мероприятии @EventId</MudText>

@code {

    /// <summary>
    /// Идентификатор мероприятия в рамках конкурса.
    /// </summary>
    [Parameter]
    public string? EventId { get; set; }
}
