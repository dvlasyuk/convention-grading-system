@page "/forms/event-attendance/{eventId}"
@layout BaseLayout

@using ConventionGradingSystem.Blazor.Components.Shared
@using Microsoft.AspNetCore.Components.Web
@using MudBlazor

<PageTitle>Форма посещаемости мероприятия @EventId</PageTitle>
<MudText Typo="Typo.h6">Форма посещаемости мероприятия @EventId</MudText>

@code {

    /// <summary>
    /// Идентификатор мероприятия в рамках конкурса.
    /// </summary>
    [Parameter]
    public string? EventId { get; set; }
}
